my.func<-function(mean.mpg){
data(mtcars)
mpg<-mtcars$mpg
sd.mpg<-input$value
#wektor gęstości
d.vec<-dnorm(mpg, mean.mpg, sd.mpg)
#log-wiarygodność
sum(log(d.vec))
}
points.lik<-seq(10,30,by=.01)
lik.points<-rep(NA,length(points.lik))
for (i in 1:length(points.lik)) lik.points[i]<-my.func(points.lik[i])
my.lab<-"średnia"
input$type=="średnia"
